{
  "version": 2,
  "rewrites": [
    { "source": "/styles.css", "destination": "/public/styles.css" },
    { "source": "/script.js", "destination": "/public/script.js" },
    { "source": "/noticias.json", "destination": "/public/noticias.json" },
    { "source": "/images/(.*)", "destination": "/public/images/$1" },
    { "source": "/(.*)", "destination": "/api/server.js" }
  ]
}